{
  "name": "nansen-profiler",
  "version": "1.0.0",
  "description": "On-chain wallet investigation tool using Nansen API",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "dev": "tsx src/index.ts",
    "analyze": "tsx src/analyze.ts",
    "comprehensive": "tsx src/comprehensive-analysis.ts",
    "launch-window": "tsx src/launch-window-analysis.ts",
    "test:related": "tsx src/test-related-wallets.ts",
    "test:counterparties": "tsx src/test-counterparties.ts",
    "test:transactions": "tsx src/test-transactions.ts",
    "test:chain": "tsx src/test-funding-chain.ts",
    "timing": "tsx src/timing-analysis.ts",
    "alt-paths": "tsx src/alternative-paths.ts",
    "network": "tsx src/network-graph.ts",
    "tokens": "tsx src/token-analysis.ts",
    "balance": "tsx src/historical-balance.ts",
    "signers": "tsx src/signer-analysis.ts",
    "pre-launch": "tsx src/pre-launch-analysis.ts",
    "pre-launch:report": "tsx src/pre-launch-analysis.ts --report-only",
    "verify": "tsx src/deep-verification.ts",
    "complete": "tsx src/complete-investigation.ts",
    "insider": "tsx src/insider-hunt-v2.ts",
    "insider-v3": "tsx src/insider-detection-v3.ts",
    "status": "tsx src/quick-status.ts",
    "predict": "tsx src/launch-prediction.ts",
    "thorough-insider": "tsx src/thorough-insider-investigation.ts",
    "start": "node dist/index.js"
